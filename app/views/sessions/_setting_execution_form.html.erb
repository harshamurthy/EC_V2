<div class="workout-session-setting-form">
  <%= form_for(setting_execution, remote: true, :html => { :class => 'session-setting-execution-form' }) do |f| %>
    <% if setting_execution.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(setting_execution.errors.count, "error") %> prohibited this setting_execution from being saved:</h2>

        <ul>
        <% setting_execution.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <% if exercise.seat? %>
      <div class="field">
        <%= f.label :seat %><br>
        <%= f.text_field :seat %>
      </div>
    <% end %>

    <% if exercise.foot? %>

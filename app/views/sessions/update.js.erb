// Gives user feedback that a coach has been selected for the session
$('.coach-select-container i').remove();
$('.coach-select-container').prepend("<i class=\"fa fa-check-square-o\"></i>");

// Gives user feedback that no coach has been selected for the session
if($('#sessionCoach').val() === '') {
  $('.coach-select-container i').remove();
  $('.coach-select-container').prepend("<i class=\"fa fa-exclamation-triangle choose-coach-label\"></i>")
}

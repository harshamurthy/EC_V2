          <%= f.label :reps %>
        </div>
      <% end %>

      <% if exercise.time? %>
        <div class="field">
          <%= f.text_field :time, placeholder: "Time" %><br>
          <%= f.label :time %>
        </div>
      <% end %>

      <% if exercise.seat? %>
        <div class="field">
          <%= f.text_field :seat, placeholder: "Seat" %><br>
          <%= f.label :seat %>
        </div>
      <% end %>

      <% if exercise.foot? %>
        <div class="field">

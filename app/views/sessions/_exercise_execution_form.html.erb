<div class="workout-session-execution-form">
  <%= form_for(exercise_execution, remote: true) do |f| %>
    <p class='save-exercise-execution-alert hide'>Successfully saved!</p>
      <% if exercise.weight? %>
        <div class="field">
          <%= f.text_field :weight, placeholder: "Weight" %><br>
          <%= f.label :weight %>
        </div>
      <% end %>

      <% if exercise.percentage? %>
        <div class="field">
          <%= f.text_field :percentage, placeholder: "Percentage" %><br>
          <%= f.label :percentage %>
        </div>
      <% end %>

      <% if exercise.reps? %>
        <div class="field">

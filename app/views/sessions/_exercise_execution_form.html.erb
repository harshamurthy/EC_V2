      <% if exercise.foot_stool? %>
        <div class="field">
          <%= f.text_field :foot_stool, placeholder: "Foot stool" %><br>
          <%= f.label :foot_stool %>
        </div>
      <% end %>

      <% if exercise.pin? %>
        <div class="field">
          <%= f.text_field :pin, placeholder: "Pin" %><br>
          <%= f.label :pin %>
        </div>
      <% end %>

      <% if exercise.arm? %>
        <div class="field">
          <%= f.text_field :arm, placeholder: "Arm" %><br>

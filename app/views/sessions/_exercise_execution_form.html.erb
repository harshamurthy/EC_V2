          <%= f.text_field :leg, placeholder: "Leg" %><br>
          <%= f.label :leg %>
        </div>
      <% end %>

      <%#= exercise.name %>
      <%= f.hidden_field :exercise_id, value: exercise.id %>
      <%= f.hidden_field :session_id, value: session.id %>
      <div class="actions">
        <%= f.submit 'Save Workout Session', class: '' %>
    </div>
  <% end %>
</div>

      <% if exercise.knee? %>
        <div class="field">
          <%= f.text_field :knee, placeholder: "Knee" %><br>
          <%= f.label :knee %>
        </div>
      <% end %>

      <% if exercise.seatpad? %>
        <div class="field">
          <%= f.text_field :seatpad, placeholder: "Seatpad" %><br>
          <%= f.label :seatpad %>
        </div>
      <% end %>

      <% if exercise.chestpad? %>
        <div class="field">
          <%= f.text_field :chestpad, placeholder: "Chestpad" %><br>
          <%= f.label :chestpad %>
        </div>
      <% end %>

      <% if exercise.hips? %>
        <div class="field">
          <%= f.text_field :hips, placeholder: "Hips" %><br>

$('.workout-session-setting-form').remove();

$('.workout-session-execution-data').prepend("<%= j render('sessions/setting_execution_form', exercise: @exercise, setting_execution: @setting_execution, client: @session.client, session: @session) %>");

$('.workout-session-execution-form input[type=text]').focusout(function(){
  $('.session-exercise-execution-form').submit();
});

$('.workout-session-setting-form input[type=text]').focusout(function() {
  $('.session-setting-execution-form').submit();
});

$("<p class='session-saved'> Saved. </p>").hide().appendTo('.workout-session-execution-data').fadeIn("fast", function(){
  $(this).delay(1000).fadeOut("slow");
});

<% if @n.client.notes.first == @n %>
  $('.no-notes-note-container').remove();
  $('.no-note-img-container').remove();

  $('.client-note-container').prepend("<%= j render('client_note_form', client: @n.client) %>");
  $('.client-note-container').append("<ul></ul>");
<% end %>


<% if params[:client_page] %>
  $('.client-note-container ul').prepend("<%= j render('note', note: @n) %>");
  $('.client-note-container input[type=text]').val("");
<% else %>
  $('.session-notes ul').prepend("<%= j render('note', note: @n) %>");
  $('.session-note-form input[type=text]').val("");
  $('.workout-session-execution-data').css("position", "absolute");
<% end %>
